{
  "name": "cozy-mobile",
  "description": "This is the native mobile client for Cozy.",
  "version": "0.1.3",
  "engines": {
    "node": "~0.10"
  },
  "scripts": {
    "postinstall": "./node_modules/.bin/bower install --config.interactive=false && ./node_modules/.bin/brunch build",
    "test": "./node_modules/.bin/mocha test/index.coffee --reporter spec --compilers coffee:coffee-script/register --colors",
    "start": "./node_modules/.bin/brunch watch --server"
  },
  "dependencies": {
    "auto-reload-brunch": "> 1.0 < 1.5",
    "bower": "1.6.7",
    "brunch": "1.8.5",
    "clean-css-brunch": "> 1.0 < 1.5",
    "coffee-script-brunch": "> 1.0 < 1.5",
    "css-brunch": "> 1.0 < 1.5",
    "jade-brunch": "> 1.0 < 1.5",
    "javascript-brunch": "> 1.0 < 1.5",
    "json-brunch": "^1.5.4",
    "stylus-brunch": "^1.8.1",
    "uglify-js-brunch": "> 1.0 < 1.5"
  },
  "devDependencies": {
    "coffee-script": "1.10.0",
    "mocha": "2.3.4",
    "should": "7.1.1",
    "chai": "1.7.2",
    "expect.js": "0.1.2",
    "ws": "0.4.21"
  }
}
